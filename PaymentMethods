            <button id="chat-abuse-report-btn"
                    class="btn-primary-xs"
                    ng-click="abuseReport(null, true)">
                Report
            </button>

            <button class="btn-control-xs"
                    ng-click="dialogLayout.isConfirmationOn = false">
                Cancel
            </button>
        </div>
    </div>
</script>

